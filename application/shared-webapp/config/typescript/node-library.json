{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node JS Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "ES2023"
    ],
    "noEmit": false,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true
  }
}
