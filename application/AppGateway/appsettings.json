{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "blob-storage": "UseDevelopmentStorage=true"
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "favicon": {
        "ClusterId": "account-static",
        "Match": {
          "Path": "/favicon.ico"
        },
        "Transforms": [
          {
            "ResponseHeader": "Cache-Control",
            "Set": "public, max-age=604800"
          },
          {
            "ResponseHeader": "Content-Type",
            "Set": "image/x-icon"
          }
        ]
      },
      "apple-touch-icon": {
        "ClusterId": "account-static",
        "Match": {
          "Path": "/apple-touch-icon.png"
        },
        "Transforms": [
          {
            "ResponseHeader": "Cache-Control",
            "Set": "public, max-age=604800"
          }
        ]
      },
      "manifest": {
        "ClusterId": "account-static",
        "Match": {
          "Path": "/manifest.json"
        },
        "Transforms": [
          {
            "ResponseHeader": "Cache-Control",
            "Set": "public, max-age=604800"
          },
          {
            "ResponseHeader": "Content-Type",
            "Set": "application/manifest+json"
          }
        ]
      },
      "root": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/"
        }
      },
      "tracking": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/api/track"
        }
      },
      "avatars": {
        "ClusterId": "account-storage",
        "Match": {
          "Path": "/avatars/{**catch-all}"
        },
        "Transforms": [
          {
            "ResponseHeader": "Cache-Control",
            "Set": "public, max-age=2592000, immutable"
          }
        ]
      },
      "logos": {
        "ClusterId": "account-storage",
        "Match": {
          "Path": "/logos/{**catch-all}"
        },
        "Transforms": [
          {
            "ResponseHeader": "Cache-Control",
            "Set": "public, max-age=2592000, immutable"
          }
        ]
      },
      "account-api": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/api/account/{**catch-all}"
        }
      },
      "account-spa": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/account/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "account-static": {
        "ClusterId": "account-static",
        "Match": {
          "Path": "/account/static/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "static/{**catch-all}"
          }
        ]
      },
      "account-hmr": {
        "ClusterId": "account-static",
        "Match": {
          "Path": "/account/{filename}.hot-update.{ext}"
        },
        "Transforms": [
          {
            "PathPattern": "/{filename}.hot-update.{ext}"
          }
        ]
      },
      "account-admin": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/admin/{**catch-all}"
        }
      },
      "account-error": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/error"
        }
      },
      "account-login": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/login/{**catch-all}"
        }
      },
      "account-signup": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/signup/{**catch-all}"
        }
      },
      "account-legal-documents": {
        "ClusterId": "account-static",
        "Match": {
          "Path": "/legal/documents/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "legal/documents/{**catch-all}"
          },
          {
            "ResponseHeader": "Content-Type",
            "Set": "text/markdown; charset=utf-8"
          },
          {
            "ResponseHeader": "Cache-Control",
            "Set": "no-cache, no-store, must-revalidate"
          }
        ]
      },
      "account-legal-pages": {
        "ClusterId": "account-api",
        "Match": {
          "Path": "/legal/{**catch-all}"
        }
      },
      "back-office-api": {
        "ClusterId": "back-office-api",
        "Match": {
          "Path": "/api/back-office/{**catch-all}"
        }
      },
      "back-office-spa": {
        "ClusterId": "back-office-api",
        "Match": {
          "Path": "/back-office/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "back-office-static": {
        "ClusterId": "back-office-static",
        "Match": {
          "Path": "/back-office/static/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "static/{**catch-all}"
          }
        ]
      },
      "back-office-hmr": {
        "ClusterId": "back-office-static",
        "Match": {
          "Path": "/back-office/{filename}.hot-update.{ext}"
        },
        "Transforms": [
          {
            "PathPattern": "/{filename}.hot-update.{ext}"
          }
        ]
      }
    },
    "Clusters": {
      "account-api": {
        "Destinations": {
          "destination": {
            "Address": "https://localhost:9100"
          }
        }
      },
      "account-static": {
        "Destinations": {
          "destination": {
            "Address": "https://localhost:9101"
          }
        }
      },
      "back-office-api": {
        "Destinations": {
          "destination": {
            "Address": "https://localhost:9200"
          }
        }
      },
      "back-office-static": {
        "Destinations": {
          "destination": {
            "Address": "https://localhost:9201"
          }
        }
      },
      "account-storage": {
        "Destinations": {
          "destination": {
            "Address": "http://127.0.0.1:10000/devstoreaccount1"
          }
        }
      }
    }
  }
}
